	author = {Stall, Shelley and Cruse, Patricia and Cousijn, Helena and Cutcher-Gershenfeld, Joel and Waard, Anita de and Hanson, Brooks and Heber, Joerg and Lehnert, Kerstin and Parsons, Mark and Robinson, Erin and Witt, Michael and Wyborn, Lesley and Yarmey, Lynn},
