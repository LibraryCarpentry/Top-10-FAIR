@article{stall_data_2018,
	title = {Data {Sharing} and {Citations}: {New} {Author} {Guidelines} {Promoting} {Open} and {FAIR} {Data} in the {Earth}, {Space}, and {Environmental} {Sciences}},
	volume = {41},
	shorttitle = {Data {Sharing} and {Citations}},
